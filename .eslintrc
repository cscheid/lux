{
	"rules": {
		"indent": [
			2,
			2,
			{ "SwitchCase": 1 }
		],
		"quotes": [
			2,
			"single"
		],
		"linebreak-style": [
			2,
			"unix"
		],
		"semi": [
			2,
			"always"
		],
		"max-len": [
			2,
			80
		],
		"no-console": 0
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
    "ecmaFeatures": {
        "modules": true,
        "impliedStrict": true,
        "experimentalObjectRestSpread": true
    },
	"extends": "eslint:recommended"
}
